<Window x:Class="Task1WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task1WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <DockPanel LastChildFill="False">
            <Label>Left num: </Label>
            <TextBox Width="100" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Name="rightNumTxtBox">0</TextBox>
            <Label>Right num: </Label>
            <TextBox Width="100" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Name="leftNumTxtBox">100</TextBox>
            <Button x:Name="startBtn" Width="200" Click="startBtn_Click">Start</Button>
        </DockPanel>
        <ListBox x:Name="elemsListBox" Grid.Row="1"></ListBox>
    </Grid>
</Window>
